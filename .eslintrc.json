{
  "extends": ["react-app", "plugin:jsx-a11y/recommended", "prettier"],
  "plugins": ["prettier", "jsx-a11y"],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
